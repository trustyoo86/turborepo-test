{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "deploy:build": "open-next build",
    "deploy": "sst deploy"
  },
  "devDependencies": {
    "@types/eslint": "^8",
    "aws-cdk-lib": "^2.83.1",
    "constructs": "10.1.156",
    "eslint": "^8.30.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "sst": "^2.13.1",
    "turbo": "latest",
    "typescript": "^4.9.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "apps/**/*.{js,ts,jsx,tsx}": [],
    "packages/ui/**/*.{js,ts,jsx,tsx}": []
  },
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "open-next": "1.4.0"
  }
}